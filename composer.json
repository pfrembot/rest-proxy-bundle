{
    "name": "pfrembot/rest-proxy-bundle",
    "type": "symfony-bundle",
    "description": "Symfony 2 bundle for using generated API entity classes",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "nikic/php-parser": "^1.0",
        "jms/serializer-bundle": "^0.13 || ^1.0",
        "symfony/framework-bundle": "^2.3 || ^3.0"
    },
    "require-dev": {
        "php": "^5.6 || ^7.0",
        "phpunit/phpunit": "^5.3",
        "mockery/mockery": "^0.9.4",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pfrembot\\RestProxyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pfrembot\\RestProxyBundle\\Tests\\": "tests/"
        }
    }
}
